{
    "name": "thigas88/alfresco-api-php-client",
    "type": "library",
    "description": "A PHP library for Alfresco API ",
    "keywords": [
        "php",
        "alfresco",
        "api"
    ],
    "homepage": "",
    "license": ["GPL-2.0-only"],
    "authors": [
        {
            "name": "Thiago Mendes Borges",
            "homepage": "https://github.com/thigas88"
        }
    ],
    "support": {
		"issues": "https://github.com/thigas88/alfresco-api-php-client/issues",
		"source": "https://github.com/thigas88/alfresco-api-php-client",
        "docs": "https://github.com/thigas88/alfresco-api-php-client/docs"
	},
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Alfresco\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Alfresco\\" : "test/" }
    }
}
